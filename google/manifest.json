{
  "name": "邦客",
  "description": "这是一款为淘宝掌柜开发的可疑订单识别插件",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/bangke_axios.js",
        "js/bangke_jquery.js",
        "js/bangke_jquery.liMarquee.js",
        "js/bangke_vue.js",
        "js/bangke_aes.js",
        "js/jcz_qcc_pagination.js",
        "js/jcz_qcc_mode-ecb.js",
        "js/bangke_changesoldlist.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/bangke_jquery.js",
      "js/bangke_axios.js",
      "js/bangke_aes.js",
      "js/jcz_qcc_mode-ecb.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "cookies",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "unlimitedStorage",
    "notifications",
    "storage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "<all_urls>"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' ;",
  "web_accessible_resources": [
    "img/*",
    "css/*",
    "js/*"
  ],
  "icons": {
    "128": "img/bangke_logo_128.png",
    "48": "img/bangke_logo_48.png",
    "16": "img/bangke_logo_16.png"
  }
}
